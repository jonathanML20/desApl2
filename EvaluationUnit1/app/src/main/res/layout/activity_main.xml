<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal"
    tools:context="com.utng.jonaml.evaluationunit1.MainActivity">

    <TableRow>
    <ImageView android:id="@+id/imv_calc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/calc1"
        android:onClick="calcActivity"
        />

    </TableRow>

    <TableRow>
    <ImageView android:id="@+id/imv_calc1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/frame"
        android:onClick="formActivity"
        />
    </TableRow>
</TableLayout>
